{
  "name": "resq-link",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "cross-env NEXT_PUBLIC_API_BASE_URL=http://localhost:4000 NEXT_PUBLIC_WEB_BASE_URL=http://localhost:3000 turbo run dev --filter=@resq-link/web --filter=@resq-link/api --filter=@resq-link/mobile",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:web": "pnpm --filter @resq-link/web exec playwright test",
    "test:web:ui": "pnpm --filter @resq-link/web exec playwright test --ui",
    "test:web:headed": "pnpm --filter @resq-link/web exec playwright test --headed",
    "test:web:debug": "pnpm --filter @resq-link/web exec playwright test --debug",
    "check": "pnpm lint && pnpm test",
    "prepare": "husky install",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "pnpm exec eslint --fix",
    "*.{json,md,css,scss,ts,tsx,js,jsx}": "pnpm exec prettier --write"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.23",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-jest": "^29.7.0",
    "babel-preset-expo": "54.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.13",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "turbo": "^2.5.8",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.46.2"
  },
  "pnpm": {
    "overrides": {
      "prettier": "^3.6.2"
    }
  }
}
